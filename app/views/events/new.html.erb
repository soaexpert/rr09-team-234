<% form_for @event, :html => { :multipart => true } do |f| %>
  <textfield>
      <%= f.label :label %>
      <%= f.text_field :label %>
      
      <%= f.label :date %>
      <input class="datetimepicker" name="event[date]"/>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :logo %>
      <%= f.file_field :logo %>

      <%= f.label :description %>
      <%= f.text_area :description%>

      <%= f.submit 'send' %>
  </fieldset>
<% end %>